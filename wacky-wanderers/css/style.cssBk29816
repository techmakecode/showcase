.navigation { float: right; margin: 0; position: fixed; right: -400px; top: 0; width: 400px; background-color: #fff; height: 100%; padding: 0; box-sizing: border-box; z-index: 5; transition: all 0.5s ease-in-out; -webkit-transition: all 0.5s ease-in-out; -ms-transition: all 0.5s ease-in-out; -moz-transition: all 0.5s ease-in-out; -o-transition: all 0.5s ease-in-out; }
.navigation a.close { width: 20px; height: 20px; position: absolute; right: 25px; top: 35px; float: right; }
