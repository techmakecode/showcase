.clearfix {/*display: inline-block;*/ }
* html .clearfix { height: 1%; }
* { -webkit-tap-highlight-color: rgba(0, 0, 0, 0); border: none; -webkit-border-radius: 0; }
.clear { font-size: 1%; height: 0; line-height: 0; width: 0; float: none; clear: both; }
html[xmlns] .clearfix {/*display: block;*/ }
input { outline: none; -webkit-appearance: none; }
article, aside, canvas, details, figcaption, figure, footer, header, hgroup, menu, nav, section, summary, mark { display: block; width: 100%; }
body { margin: 0; padding: 0; font-family: 'Lato'; }
a { transition: all 0.2s ease-in-out; -webkit-transition: all 0.2s ease-in-out; -moz-transition: all 0.2s ease-in-out; -ms-transition: all 0.2s ease-in-out; -o-transition: all 0.2s ease-in-out; }
