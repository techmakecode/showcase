#journey3 { background: #00abd7; overflow: hidden }
#journey1 .skating .mountainWrp, #journey1 .skating .sunshadow, #journey1 .skating .hillWrp, #journey1 .skating .treeWrap.layb, #journey1 .skating .snowLay, #journey1 .skating .treeWrap.layf, #journey1 .skating .snowbreeze, #journey1 .skating .snowball, #journey1 .skating .snowball .svgImg, #journey1 .skating .snowball:after, #journey1 .skating .blast, #journey1 .skating .blast .svgImg, #journey1 .skating .explod, #journey1 .skating .explod .svgImg, #journey1 .skating .skiingWrap, #journey1 .skating .skiin { position: absolute; }
#journey1 .skating, #journey1 .skating .mountainWrp, #journey1 .skating .treeWrap.layb, #journey1 .skating .snowLay, #journey1 .skating .treeWrap.layf, #journey1 .skating .snowball .svgImg { width: 100%; height: 100% }
#journey1 .skating { float: left; overflow: hidden; background: #A5D9CA; position: relative }
/*Clouds Css Start*/
#journey1 .skating .cloudsWrp { float: left; width: 100%; height: 192px; position: absolute; top: 0; left: 0; }
#journey1 .skating .cloud1, #journey1 .skating .cloud2 { animation: cloud1 10s linear infinite; -webkit-animation: cloud1 10s linear infinite; -moz-animation: cloud1 10s linear infinite; -ms-animation: cloud1 10s linear infinite; -o-animation: cloud1 10s linear infinite; -webkit-perspective: 1000; }
#journey1 .skating .cloud2 { animation-name: cloud2; -webkit-animation-name: cloud2; -moz-animation-name: cloud2; -ms-animation-name: cloud2; -o-animation-name: cloud2; }
