.navigation li a:before { position: absolute; top: 50%; left: 0; content: ""; height: 1px; background-color: #70a2c1; width: 0; transition: all 0.2s ease-in-out; -webkit-transition: all 0.2s ease-in-out; -moz-transition: all 0.2s ease-in-out; -ms-transition: all 0.2s ease-in-out; -o-transition: all 0.2s ease-in-out; }
.navigation li a:hover:before { width: 100px; }
.navigation li a.active { font-weight: 700; }
.navigation li a.active:before { width: 20px; left: 80px; background-color: #025b92; }
.navigation li:nth-child(4) { display: none; }
.navigation .box .socialLinks { position: absolute; bottom: 20px; width: 100%; left: 0; text-align: center; font-size: 15px; color: #025b92; }
.navigation .box .socialLinks span { position: relative; top: -6px; right: 10px; }
