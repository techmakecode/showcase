.navigation .box { width: 100%; height: 100%; display: table; }
.navigation .box .content { width: 100%; height: 100%; display: table-cell; vertical-align: middle; }
.navigation li { float: left; width: 100%; transition: all 0.7s ease-in-out; -webkit-transition: all 0.7s ease-in-out; -moz-transition: all 0.7s ease-in-out; -ms-transition: all 0.7s ease-in-out; -o-transition: all 0.7s ease-in-out; transition-delay: 0.1s; -webkit-transition-delay: 0.1s; -moz-transition-delay: 0.1s; -ms-transition-delay: 0.1s; -o-transition-delay: 0.1s; margin-bottom: 55px; }
